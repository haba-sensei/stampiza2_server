{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "ES2022",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "strict": true,
        "skipLibCheck": true,
        "explainFiles": true,
        "baseUrl": ".", 
    }, 
     "include": ["src/**/*.ts"],
     "exclude": ["node_modules"] 
}